{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "quotes": [2, "single"],
    "variable-name": {
        "options": [
          "ban-keywords",
          "check-format",
          "allow-leading-underscore",
          "allow-pascal-case",
          "allow-snake-case"
        ]
      }
  },
  "rulesDirectory": []
}
